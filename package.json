  "bugs": {
    "url": "https://github.com/photonstorm/phaser3-custom-build/issues"
  },
  "homepage": "https://github.com/photonstorm/phaser3-custom-build#readme",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "eventemitter3": "^3.1.0",
    "phaser": "^3.11.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
